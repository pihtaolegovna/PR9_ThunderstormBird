<ui:UiWindow x:Class="PR9_ThunderstormBird.Auth"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PR9_ThunderstormBird"
        mc:Ignorable="d"
        Title="Auth" Height="250" Width="300"
		xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
		xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
		d:DesignHeight="250"
		d:DesignWidth="300"
		Background="DimGray"
		ExtendsContentIntoTitleBar="True"
		WindowBackdropType="Mica"
		WindowCornerPreference="Round"
		WindowStartupLocation="CenterScreen">
	<Window.Resources>
		<Storyboard x:Key="myAnimation">
			<DoubleAnimation Storyboard.TargetName="myColumn" Storyboard.TargetProperty="Width" From="0" To="200" Duration="0:0:1"/>
		</Storyboard>
	</Window.Resources>
	<Grid Margin="15,15,15,15" HorizontalAlignment="Stretch">
		<WrapPanel Width="240" HorizontalAlignment="Stretch">
			<ui:TextBox/>
		</WrapPanel>
	</Grid>
</ui:UiWindow>
